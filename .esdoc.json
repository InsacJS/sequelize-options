{
  "source": "./lib",
  "destination": "./docs/",
  "includes": ["\\.js$"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "undocumentIdentifier": { "enable": false },
        "unexportedIdentifier": { "enable": true },
        "lint": { "enable": true },
        "coverage": { "enable": true },
        "typeInference": { "enable": true },
        "brand": {
          "title": "Sequelize Options",
          "description": "Facilita la creaci√≥n del objeto options que utiliza Sequelize para realizar consultas a la base de datos.",
          "repository": "https://github.com/waquispe/sequelize-options"
        }
      }
    }
  ]
}
